generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model paletas {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  sabor     String  @unique
  descricao String  @unique
  foto      String  @unique
  preco     Int
}